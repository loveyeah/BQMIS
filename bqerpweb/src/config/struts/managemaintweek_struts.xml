<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-validation.xml" />
	<package name="managemaintweek" namespace="/managemaintweek"
		extends="jfreechart-default">
		<!--检修周计划汇总-->
		<action name="addRepairGather"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="addRepairGather">
		</action>
		<action name="delRepairGatherDetail"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="delRepairGatherDetail">
		</action>
		<action name="getRepairGatherDetailList"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="getRepairGatherDetailList">
		</action>
		<action name="RepairGatherReport"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="RepairGatherReport">
		</action>
		<action name="RepairGatherApprove"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="RepairGatherApprove">
		</action>
		<action name="getRepairApproveList"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="getRepairApproveList">
		</action>
		<action name="getRepairGatherInfo"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="getRepairGatherInfo">
		</action>
		<action name="getRepairApproveGatherId"
			class="power.web.manage.plan.maintWeekPlan.MaintWeekPlanGatherAction"
			method="getRepairApproveGatherId">
		</action>

	</package>
</struts>
