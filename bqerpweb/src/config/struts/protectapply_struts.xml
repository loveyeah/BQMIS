<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<include file="struts-validation.xml" />
	<package name="protect" namespace="/protect"
		extends="jfreechart-default">

		<!-- 保护投退申请单登记 -->
		<action name="addProtectApply"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="addProtectApply">
		</action>
		<action name="updateProtectApply"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="updateProtectApply">
		</action>
		<action name="deleteProtectApply"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="deleteProtectApply">
		</action>
		<action name="findByIsinList"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="findByIsinList">
		</action>
		<action name="findByNo"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="findByNo">
		</action>
		<action name="findRegisterList"
			class="power.web.run.protectapply.action.ProtectApplyRegisterAction"
			method="findRegisterList">
		</action>
		<!-- 保护投退申请单上报及审批 -->
		<action name="findProtectApproveList"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="findProtectApproveList">
		</action>
		<action name="protectReport"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="protectReport">
		</action>
		<action name="protectCommSign"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="protectCommSign">
		</action>
		<action name="protectFactoryLeadSign"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="protectFactoryLeadSign">
		</action>
		<action name="currentChargerSign"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="currentChargerSign">
		</action>
		<action name="protectRunManSign"
			class="power.web.run.protectapply.action.ProtectReportApproveAction"
			method="protectRunManSign">
		</action>
		<!-- 保护投退申请单查询 -->
		<action name="findProtectQueryList"
			class="power.web.run.protectapply.action.ProtectApplyQueryAction"
			method="findProtectQueryList">
		</action>
	</package>
</struts>