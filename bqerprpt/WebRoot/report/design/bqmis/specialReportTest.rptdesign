            <property name="queryText">select t.display_no, to_char(riqi.s_date, 'MM-dd') tempDate,t.item_second_name, 
 decode(substr(to_char(riqi.s_date, 'MMdd'),3,1),'0',substr(to_char(riqi.s_date, 'MMdd'),4,1),substr(to_char(riqi.s_date, 'MMdd'),3,2))  itemDate,
       riqi.dis,
       t.ITEM_BASE_NAME,
       t.item_alias,
       round(decode(t.data_time_type,
       '3',
       (select decode(t.is_show_zero,'N',(decode(r.data_value,0,null,r.data_value)), r.data_value) from bp_j_stat_rtz r where r.data_date=riqi.s_date and r.item_code= t.item_code) 
       ,'4',
       (select decode(t.is_show_zero,'N',(decode(y.data_value,0,null,y.data_value)), y.data_value) from bp_j_stat_ytz y where y.data_date=riqi.s_date and y.item_code = t.item_code)
      ,'6',
      (select decode(t.is_show_zero,'N',(decode(n.data_value,0,null,n.data_value)), n.data_value) from bp_j_stat_ntz n where n.data_date=riqi.s_date and n.item_code = t.item_code) ),t.data_type) itemvalue
      
  from (select add_months(to_date(?|| '26', 'yyyymmdd'), -1) + (rownum - 1) s_date,
       rownum as dis
  from dual
connect by rownum &lt;=
           last_day(add_months(to_date(?|| '26', 'yyyymmdd'), -1)) -
           add_months(to_date(? || '26', 'yyyymmdd'), -1) + 26) riqi,
       bp_c_item_report_new t
 where t.report_code = ?
       and t.conkers_no=1
 order by to_number(t.item_second_name), t.display_no,riqi.dis
 </property>
        </oda-data-set>
