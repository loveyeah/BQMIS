            <method name="fetch"><![CDATA[if(params["appId"] != null){
if(list != null &amp;&amp;  list.size()>0){
if(count < list.size()){
						row["contractNo"]=list[count].getContractNo();
						row["clientName"]=list[count].getClientName();
						row["contractName"]=list[count].getContractName();
						row["actAccount"]=list[count].getActAccount();
						row["payedAccount"]=list[count].getPayedAccount();
						row["applicatPrice"]=list[count].getApplicatPrice();
						row["passPrice"]=list[count].getPassPrice();
						row["balance"]=list[count].getBalance();
						row["memo"]=list[count].getMemo();
						params["hjAct"].value = list[count].getHjAct()
						params["hjPay"].value = list[count].getHjPay()
						params["hjApp"].value = list[count].getHjApp()
						params["hjPass"].value = list[count].getHjPass()
						params["hjBala"].value = list[count].getHjBala()
						params["deptName"].value = list[count].getDeptName()
						params["workerName"].value = list[count].getWorkerName()
						params["moneyUnit"].value = list[count].getMoneyUnit()
						params["reprotDate"].value = list[count].getReportDate()
						row["count"]=++count;
						return true;
}
}
}
return false;]]></method>
