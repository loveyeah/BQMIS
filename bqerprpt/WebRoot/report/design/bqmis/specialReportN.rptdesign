            <property name="queryText">select t.display_no,t.item_second_name, substr(riqi.s_date,6,5) as dd, decode(length(riqi.s_date),
'7',
 decode(substr(riqi.s_date,6,1),'0',substr(riqi.s_date,7,1), substr(riqi.s_date,6,2)) || '月',
'9',
 '合计'
) itemDate,
       t.ITEM_BASE_NAME,
       t.item_alias,
       round(decode(length(riqi.s_date),
              '7',
              (select decode(t.is_show_zero,'N',(decode(y.data_value,0,null,y.data_value)), y.data_value)
                 from bp_j_stat_ytz y
                where y.data_date =
                      (select max(a.data_date)
                         from bp_j_stat_ytz a
                        where to_char(a.data_date, 'yyyy-mm') =riqi.s_date
                          and a.item_code = t.item_code)
                  and y.item_code = t.item_code),
              '9',
              (select decode(t.is_show_zero,'N',(decode(n.data_value,0,null,n.data_value)), n.data_value)
                 from bp_j_stat_ntz n
                where n.data_date =
                      (select max(b.data_date)
                         from bp_j_stat_ntz b
                        where to_char(b.data_date, 'yyyy') = ?
                          and b.item_code =t.item_code)
                  and n.item_code =t.item_code )),t.data_type)itemvalue
  from (select to_char(to_date(?|| rownum, 'yyyyMM'), 'yyyy-mm') s_date
          from dual
        connect by rownum &lt;= 12
        union
        select ? ||'-12.1' from dual) riqi,
       bp_c_item_report_new t
 where t.report_code = ?
 order by t.display_no,to_number(t.item_second_name), dd</property>
        </oda-data-set>
