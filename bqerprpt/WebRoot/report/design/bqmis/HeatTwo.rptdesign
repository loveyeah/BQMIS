    <method name="beforeFactory"><![CDATA[heatTwo = new Packages.power.web.birt.action.bqmis.Heat();
heatForm = new Packages.power.web.birt.bean.bqmis.HeatBean();
heatForm = heatTwo.setHeatBean(params["no"].value, "2");
 
        // NO
 	 	params["workticketNo"].value=heatForm.getWorkticketNo();
 	 	////<!-- 动火部门 -->
 	 	params["dhDept"].value=heatForm.getDhDept();
 	 	params["repairSpecail"].value=heatForm.getDhDept();
 		////<!-- 班组 -->
 	 	params["chargeDept"].value=heatForm.getChargeDept();
 	 	////<!-- 机组 -->
		params["machineName"].value=heatForm.getEquAttributeName();
		////<!-- 动火负责人 -->
		params["fireChargeBy"].value=heatForm.getFireChargeBy();
		////<!-- 动火地点 -->
		params["workSpace"].value=heatForm.getWorkSpace();
		////<!-- 动火工作内容 -->
		params["fireWorkContent"].value=heatForm.getFireWorkContent();
		//<!-- 计划工作时间开始 -->
		params["planStartDate"].value=heatForm.getPlanStartDate();		
		//<!-- 计划工作时间结束 -->
		params["planEndDate"].value=heatForm.getPlanEndDate();
		//五、运行应采取的安全措施：
		params["securityTitle41"].value=heatForm.getSecurityTitle41();	
		//<!-- 运行应采取的安全措施 -->
		params["safetyNeedSafetyContent"].value=heatForm.getSafetyNeedSafetyContent();
		//六、检修应采取的安全措施：
		params["securityTitle42"].value=heatForm.getSecurityTitle42();		
		//<!-- 检修应采取的安全措施 -->
		params["repairNeedSafetyContent"].value=heatForm.getRepairNeedSafetyContent();
		//<!-- 审批人签章 -->
		params["approveMan"].value=heatForm.getApproveMan();
		//<!-- 动火工作票签发人 -->
		params["fireSignMan"].value=heatForm.getFireSignMan();
		//<!-- 消防部门负责人 -->
		params["fireChargeDept"].value=heatForm.getFireChargeDept();
		//<!-- 安监部门人员 -->
		params["safetyChargeDept"].value=heatForm.getSafetyChargeDept();
		//<!-- 运行班长 -->
		params["runMonitor"].value=heatForm.getRunMonitor();
		//<!-- 值长 -->
		params["watcher"].value=heatForm.getDutyCharge();
		//<!-- 运行许可人签字 -->
		params["approveMan"].value=heatForm.getApproveMan();
		////<!-- 工作负责人（监护人） -->
		params["chargeBy"].value=heatForm.getChargeBy();
		////<!-- 消防监护人签字 (符合要求) -->
		params["fireWatcherMan1"].value=heatForm.getFireWatcherMan1();
		//<!-- 许可工作时间 -->
		if(heatForm.getAdmissionDate()!= "" &amp;&amp; heatForm.getAdmissionDate() != null){
			params["admissionDate"].value=heatForm.getAdmissionDate();
		} else {
			params["admissionDate"].value="&amp;nbsp;&amp;nbsp;&amp;nbsp;年&amp;nbsp;&amp;nbsp;月&amp;nbsp;&amp;nbsp;日&amp;nbsp;&amp;nbsp;时&amp;nbsp;&amp;nbsp;分";;
		}
		//<!-- 值班负责人签字 -->
		params["fireWatcherStaMan"].value=heatForm.getFireWatcherStaMan();
		//<!-- 动火执行人签字 -->
		params["fireExeStaMan"].value=heatForm.getFireExeStaMan();
		//<!-- 结束动火时间 -->
		if(heatForm.getFireEndDate()!= "" &amp;&amp; heatForm.getFireEndDate() != null){
			params["fireEndDate"].value=heatForm.getFireEndDate();
		} else {
			params["fireEndDate"].value="&amp;nbsp;&amp;nbsp;&amp;nbsp;年&amp;nbsp;&amp;nbsp;月&amp;nbsp;&amp;nbsp;日&amp;nbsp;&amp;nbsp;时&amp;nbsp;&amp;nbsp;分";;
		}
		//<!-- 结束动火执行人签字 -->
		params["fireExeEndMan"].value=heatForm.getFireExeEndMan();
		//<!-- 结束消防监护人签字 -->
		params["fireWatcherMan2"].value=heatForm.getFireWatcherMan3();
		//<!-- 结束动火负责人签字 -->
		params["fireEndChargeBy"].value=heatForm.getFireEndChargeBy();
		//<!-- 结束运行班长签字 -->
		params["fireWatcherEndMan"].value=heatForm.getFireWatcherEndMan();
		//<!-- 备注 -->
		params["workticketMemo"].value=heatForm.getWorkticketMemo();
		params["contentFlg"].value=heatForm.isContentFlg();
		params["delete"].value=heatForm.isDelete();
		]]></method>
